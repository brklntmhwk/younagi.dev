{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "younagi.dev",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "author": "Nagi",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "preview": "astro preview",
    "build": "astro check && astro build",
    "sync": "astro sync",
    "prebuild": "bun ./src/scripts/generate-icon-types.ts",
    "postbuild": "pagefind --site dist",
    "format": "prettier --write './src/**/*.{js,ts,json,astro}'",
    "prepare": "husky",
    "wrangler:typegen": "wrangler types",
    "wrangler:dev": "wrangler pages dev --compatibility-date=2024-05-24 --proxy 3000 -- astro dev",
    "db:generate": "drizzle-kit generate",
    "db:studio:local": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "db:migrate:local": "wrangler d1 migrations apply younagi_dev_db --local",
    "db:migrate:prod": "wrangler d1 migrations apply younagi_dev_db"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/cloudflare": "^10.2.6",
    "@astrojs/markdown-remark": "^5.1.0",
    "@astrojs/mdx": "^3.0.0",
    "@astrojs/rss": "4.0.5",
    "@astrojs/sitemap": "^3.1.4",
    "@astrojs/solid-js": "^4.2.0",
    "@fontsource-variable/inconsolata": "^5.0.18",
    "@modular-forms/solid": "^0.21.0",
    "@unocss/reset": "^0.60.0",
    "astro": "^4.10.0",
    "astro-purgecss": "4.1.0",
    "drizzle-orm": "^0.31.1",
    "purgecss": "5.0.0",
    "solid-js": "^1.8.17",
    "solid-motionone": "^1.0.0",
    "valibot": "^0.31.1",
    "wretch": "^2.9.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240603.0",
    "@nanostores/solid": "^0.4.2",
    "@pagefind/default-ui": "^1.0.4",
    "@types/better-sqlite3": "^7.6.10",
    "@types/eslint": "8.56.5",
    "@types/hast": "^3.0.4",
    "@types/katex": "0.16.7",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vanilla-extract/css": "^1.15.2",
    "@vanilla-extract/vite-plugin": "^4.0.10",
    "better-sqlite3": "^11.0.0",
    "browserslist": "^4.23.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.22.2",
    "esbuild": "^0.21.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "0.31.4",
    "hast": "^1.0.0",
    "hastscript": "^9.0.0",
    "husky": "9.0.11",
    "katex": "0.16.9",
    "lightningcss": "^1.24.1",
    "lint-staged": "15.2.2",
    "mdast": "^3.0.0",
    "mdast-util-newline-to-break": "^2.0.0",
    "mdast-util-to-hast": "^13.1.0",
    "mdast-util-to-string": "^4.0.0",
    "nanostores": "^0.10.3",
    "node-html-parser": "6.1.12",
    "pagefind": "1.0.4",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-katex": "7.0.0",
    "rehype-pretty-code": "0.13.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.0",
    "remark-math": "6.0.0",
    "satori": "^0.10.13",
    "sharp": "^0.33.3",
    "shiki": "^1.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "unfurl.js": "^6.4.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "unsplash-js": "^7.0.19",
    "vite-plugin-solid": "^2.10.2",
    "wrangler": "^3.57.2"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "packageManager": "pnpm@9.1.0"
}
