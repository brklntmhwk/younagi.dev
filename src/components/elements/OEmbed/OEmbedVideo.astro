---
import type { OEmbedVideo } from './types'
// import { sanitize } from './sanitize'

type Props = {
  data: OEmbedVideo
}

const { data } = Astro.props
---

<div
  class="pokemon-border"
  set:html={data.html}
  data-oembed
  data-oembed-video
/>
<style>
  [data-oembed-video] {
    width: 100%;
  }
</style>
