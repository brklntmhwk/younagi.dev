---
import {Icon} from "astro-icon/components";

type Props = {
  iconName: "mdi:github" | "ri:unsplash-fill" | "mdi:rss" | "pixelarticons:sun" | "pixelarticons:moon"
  width?: number
  height?: number
}

const { iconName, width, height} = Astro.props;
---

<Icon name={iconName} width={width ?? 24} height={height ?? 24} />
