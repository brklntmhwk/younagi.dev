    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "npx astro",
    "format": "prettier --write './src/**/*.{js,ts,json,astro}'",
    "prepare": "husky",
    "typegen": "npx wrangler types"
  },
  "dependencies": {
